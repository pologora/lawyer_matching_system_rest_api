{
  "name": "lawyer_matching_service",
  "version": "0.0.1",
  "description": "Platform to match clients with lawyers",
  "main": "src/server.js",
  "author": "Oleksandr Wojtyla",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --files src/server.ts",
    "start:prod": "npm run build && cross-env NODE_ENV=production node build/server.js",
    "build": "rimraf build && npx tsc",
    "test": "cross-env NODE_ENV=test jest --testPathPattern=unit\\.test\\.ts$",
    "test:integration": "npm run setup:test || npm run jest:integration",
    "jest:integration": "cross-env NODE_ENV=test  jest --testPathPattern=integration\\.test\\.ts$",
    "test-watch": "jest --watch --detectOpenHandles",
    "setup": "ts-node src/config/databaseTables/setupDatabaseTablesAndValues.ts",
    "setup:test": "cross-env NODE_ENV=test ts-node src/config/databaseTables/setupDatabaseTablesAndValues.ts"
  },
  "overrides": {
    "eslint": "^9.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.0",
    "nodemailer": "^6.9.14",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
